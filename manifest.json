{
  "name": "Memer",
  "short_name": "Memer",
  "author": "Rick Beyer",
  "homepage_url": "https://github.com/fcbeyer/memer-chrome",
  "manifest_version": 2,
  "version": "1.0.2",
  "description": "replace lazy comments in threads with the meme image referenced",
  "browser_action": {
    "default_title": "Memer",
    "default_icon": "icons/jaws16.png"
  },
  "icons": {
    "16": "icons/jaws16.png",
    "48": "icons/jaws48.png",
    "128": "icons/jaws128.png"
  },
  "content_scripts": [ {
    "js": [ "jquery.min.js", "memer.js" ],
    "css": ["memer.css"],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "web_accessible_resources": [
    "images/*.png"
  ]
}